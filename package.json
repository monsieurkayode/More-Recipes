{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "A recipe sharing web application",
  "main": "app.js",
  "scripts": {
    "migrate": "sequelize db:migrate",
    "unmigrate": "sequelize db:migrate:undo:all",
    "copy:files": "gulp",
    "server:dev": "nodemon --watch server --watch client --exec babel-node ./server/bin/www",
    "client:dev": "webpack-dev-server --config webpack.config.dev.js",
    "start:devServer": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build:server": "rm -rf dist && babel --presets es2015 server -d dist/",
    "build:client": "rm -rf build && webpack --progress --config webpack.config.dev.js",
    "client:prod": "rm -rf build && webpack --config webpack.config.prod.js",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "build:prod": "concurrently \"npm run build:server\" \"npm run client:prod\"",
    "start:dev": "npm run build && node dist/bin/www.js",
    "start": "node dist/bin/www.js",
    "heroku-postbuild": "npm run migrate && npm run build:prod",
    "test:server": "NODE_ENV=test mocha --reporter progress --timeout 10000 --compilers js:babel-core/register server/test/index.test.js",
    "test": "NODE_ENV=test nyc _mocha --report lcovonly --reporter spec --timeout 20000 server/test/index.test.js && cat ./coverage/lcov.info | coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:client": "jest /client -u --coverage",
    "test:watch": "npm run test:client -- --watch",
    "test:e2e": "NODE_ENV=test npm run unmigrate && NODE_ENV=test npm run migrate && NODE_ENV=test nightwatch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monsieurkayode/More-Recipes.git"
  },
  "keywords": [
    "Javascript",
    "Node",
    "Express",
    "React",
    "Redux",
    "Sequelize",
    "Webpack"
  ],
  "nyc": {
    "require": [
      "babel-register"
    ],
    "exclude": [
      "server/test/"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": true
  },
  "jest": {
    "setupFiles": [
      "./client/tests/utils/jestSetup.js"
    ],
    "modulePathIgnorePatterns": [
      "./client/src/reducers/index.js",
      "./client/src/store/",
      "./client/src/index.jsx",
      "./client/src/routes.jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/tests/__mocks__/fileMock.js"
    },
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "verbose": true
  },
  "author": "Kayode Adeola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/monsieurkayode/More-Recipes/issues"
  },
  "homepage": "https://github.com/monsieurkayode/More-Recipes#readme",
  "devDependencies": {
    "axios-mock-adapter": "^1.12.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
