    "coveralls": "^2.13.3",
    "cross-env": "^5.0.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.5.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "expect": "^22.1.0",
    "jest": "^22.1.4",
    "jsdom": "^11.6.2",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.1.0",
